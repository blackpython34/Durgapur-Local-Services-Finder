{
  "hosting": {
    "public": "out",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/provider/**",
        "destination": "/provider/1.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
